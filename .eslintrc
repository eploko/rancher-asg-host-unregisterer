{
    "rules": {
        "indent": [2, 2],
        "quotes": [2, "single"],
        "linebreak-style": [2, "unix"],
        "new-cap": 0,
        "no-trailing-spaces": 0,
        "semi": [2, "always"],
        "object-curly-spacing": 0,
        "no-constant-condition": 0,
        "no-console": 0,
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "airbnb",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ]
}
